.nbs-flexisel-container
    :position relative
    :max-width 100%
    /*:height 650px!important*/
    /*:overflow hidden!important*/

.nbs-flexisel-ul
    :position relative
    :width 99999px
    :height 650px
    :overflow hidden
    :margin 0px
    :padding 0px
    :list-style-type none
    :text-align center

.nbs-flexisel-inner
    :overflow hidden
    :float left
    :width 100%
    /*:background #fcfcfc*/
    :border 1px solid #ccc
    :border-radius 5px
    :-moz-border-radius 5px
    :-webkit-border-radius 5px



.nbs-flexisel-item
    :float left
    :margin 0px
    :padding 0px
    :cursor pointer
    :position relative
    :line-height 0px
    img
        :width 100%
        :cursor pointer
        :position relative
        /*:margin-top 10px*/
        /*:margin-bottom 10px*/
        /*:max-width 100px*/
        /*:max-height 405px*/


/*** Navigation ***/

.nbs-flexisel-nav-left,
.nbs-flexisel-nav-right
    :width 22px
    :height 22px
    :position absolute
    :cursor pointer
    :z-index 100
    :opacity 0.5


.nbs-flexisel-nav-left
    :left 10px
    :background url("<%= asset_path('button-previous.png') %>")no-repeat

.nbs-flexisel-nav-right
    :right 5px
    :background url("<%= asset_path('button-next.png') %>")no-repeat