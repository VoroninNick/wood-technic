.production-bar-wrapper
    .columns
        :padding 0!important
    a
        :display block
        :height 100%
        :width 100%

.prod-bar
    :position relative
    :height 600px
    .prod-bar-content
        :position relative
        :top 25%
        :height 280px
        :width 240px
        :margin 0 auto
        h1
            :font-size 3rem
            :font-weight 700
            :text-shadow 1px 1px 1px rgba(0, 0, 0, 0.3)
        h3
            :font-size 1.5rem
        h1, h4
            :color #fff
        h1, h4, a
            :text-align center
        .ltc
            :position absolute
            :bottom 0
            :width 100%

            span
                :display block
                :text-align center
                :color #fff
                :border 1px solid #fff
                :font-size 1.875rem
                :padding 13px 20px

            span:hover
                :background #94d60a
                :border none
                :padding 15px 22px
    .blur-opa
        :width 100%
        :height 100%
        :position absolute
        :display block
        :-webkit-filter blur(3px)
        :-moz-filter blur(3px)
        :-o-filter blur(3px)
        :-ms-filter blur(3px)
        :filter blur(3px)
        :background rgba(255, 255, 255, 0.35)
        :transition 0.4s ease-out all
        :-webkit-transition 0.4s ease-out all

.prod-bar:hover .blur-opa
    :background none
    :-webkit-filter blur(0px)
    :-moz-filter blur(0px)
    :-o-filter blur(0px)
    :-ms-filter blur(0px)
    :filter blur(0px)

.prod-bar:hover:before
    :content ''
    :display block
    :background url("<%= asset_path('Shadow-production-left.png') %>")no-repeat
    :height 100%
    :width 15px
    :position absolute
    :left -13px
    :top 0
    :background-size 90%
    :z-index 11
.prod-bar:hover:after
    :content ''
    :display block
    :background url("<%= asset_path('Shadow-production.png') %>")no-repeat
    :height 100%
    :width 15px
    :position absolute
    :top 0
    :right -15px
    :background-size 90%
    :z-index 11

